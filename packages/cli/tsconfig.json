{
  "compilerOptions": {
    "target": "ES2022",
    "rootDir": "./src",
    "module": "commonjs",
    "resolveJsonModule": true,
    "strict": true,
    "outDir": "../../dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*"]
}
