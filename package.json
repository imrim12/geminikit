{
  "name": "geminikit",
  "version": "0.1.2",
  "private": false,
  "description": "Gemini CLI Workspace Kit",
  "workspaces": [
    ".gemini/**",
    "packages/*"
  ],
  "author": "THECODEORIGIN CO.,LTD",
  "license": "MIT",
  "homepage": "https://github.com/thecodeorigin/geminikit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecodeorigin/geminikit.git"
  },
  "bugs": {
    "url": "https://github.com/thecodeorigin/geminikit/issues"
  },
  "keywords": [
    "gemini",
    "cli",
    "agent",
    "kit",
    "monorepo"
  ],
  "bin": {
    "gk": "dist/index.js"
  },
  "files": [
    ".gemini",
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "generate-manifest": "bun scripts/generate-manifest.ts",
    "prepack": "bun run generate-manifest && bun run -F @thecodeorigin/geminikit-cli prepack"
  },
  "dependencies": {
    "@google-cloud/vertexai": "^1.10.0",
    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.0.1",
    "debug": "^4.4.0",
    "dotenv": "^16.4.5",
    "mime-types": "^2.1.35",
    "puppeteer": "^24.15.0",
    "yargs": "^17.7.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.6.1",
    "@types/bun": "^1.3.4",
    "@types/debug": "^4.1.12",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^25.0.1",
    "@types/yargs": "^17.0.35",
    "bun-types": "latest",
    "eslint": "^9.39.2",
    "typescript": "^5.5.0"
  }
}
